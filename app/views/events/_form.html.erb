<%= form_for @event do |f| %>

<%= render 'layouts/error', object: f.object %>

  <p>
    <%= f.label :title %>
    <%= f.text_field :title  %>
  </p>

  <p>
    <%= f.label :street_number %>
    <%= f.text_field  :street_number, :id => "street_number"%>
  </p>
  <p>
    <%= f.label :route %>
    <%= f.text_field :route, :id => "route" %>
  </p>

  <p>
    <%= f.label :city %>
    <%= f.text_field :city, :id => "locality"  %>
  </p>

  <p>
    <%= f.label :provence %>
    <%= f.text_field :provence, :id => "administrative_area_level_1"%>
  </p>
  <p>
    <%= f.label :country %>
    <%= f.text_field :country, :id => "country" %>
  </p>
  <p>
    <%= f.label :postal_code %>
    <%= f.text_field :postal_code, :id => "postal_code"%>
  </p>


  <p>
    <%= f.hidden_field :lat %>
  </p>
  <p>
    <%= f.hidden_field :lng %>
  </p>


  <p>
    <%= f.label :date%>
    <%= f.date_field :date %>
  </p>

  <p>
    <%= f.label :time %>
    <%= f.time_field :time %>
  </p>

  <p>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </p>

  <p>
    <%= f.submit "Submit"%>
  </p>

<% end %>
